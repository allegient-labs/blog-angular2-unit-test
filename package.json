{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "testhtmloutput": "live-server --open=unit-tests.html",
    "testjasmine": "concurrent \"npm run tsc:w\" \"npm run testhtmloutput\" ",
    "test": "tsc && karma start karma.conf.js && npm run remap-istanbul && npm run show-coverage",
    "remap-istanbul": "remap-istanbul -i ./coverage/coverage-final.json -o ./report/coverage/html-report -t html",
    "show-coverage": "live-server --open=./report/coverage/html-report/index.html"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "systemjs": "0.19.27",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "lite-server": "^2.2.0",
    "live-server": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.7",
    "remap-istanbul": "^0.6.4",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
